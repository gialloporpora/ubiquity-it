#. bold command:
#. use | to separate multiple name values:
msgctxt "bold.names"
msgid "bold"
msgstr "trasforma in grassetto"

msgctxt "bold.description"
msgid "If you're in a rich-text-edit area, makes the selected text bold."
msgstr "Trasforma il testo selezionato in grassetto se si trova in un'area di testo formattabile."

msgctxt "bold.execute"
msgid "You're not in a rich text editing field."
msgstr "Il testo selezionato non si trova di testo formattabile."


#. italicize command:
#. use | to separate multiple name values:
msgctxt "italicize.names"
msgid "italicize"
msgstr "trasmorma in corsivo"

msgctxt "italicize.description"
msgid "If you're in a rich-text-edit area, makes the selected text italic."
msgstr "Trasforma il testo selezionato in corsivo se si trova in un'area di testo formattabile."

msgctxt "italicize.execute"
msgid "You're not in a rich text editing field."
msgstr "Il testo selezionato non è all'interno di un campo di testo formattabile."


#. underline command:
#. use | to separate multiple name values:
msgctxt "underline.names"
msgid "underline"
msgstr "sottolinea"

msgctxt "underline.description"
msgid "If you're in a rich-text-edit area, underlines the selected text."
msgstr "Sottolinea il testo selezionato se si trova in un'area di testo formattabile."

msgctxt "underline.execute"
msgid "You're not in a rich text editing field."
msgstr "Il testo selezionato non è all'interno di un'area di testo formattabile."


#. highlight command:
#. use | to separate multiple name values:
msgctxt "highlight.names"
msgid "highlight|hilite"
msgstr "evidenzia"

msgctxt "highlight.description"
msgid "Highlights your current selection, like <span style=\"background: yellow; color: black;\">this</span>."
msgstr "Evidenzia il testo selezionato se si trova in un'area di testo formattabile."


#. undo (text edit) command:
#. use | to separate multiple name values:
msgctxt "undo (text edit).names"
msgid "undo"
msgstr "ripristina"

msgctxt "undo (text edit).description"
msgid "Undoes your latest style/formatting or page-editing changes."
msgstr "Rimuove le modifiche applicate alla formattazione del testo o alla pagina."

msgctxt "undo (text edit).execute"
msgid "You're not in a rich text editing field."
msgstr "You're not in a rich text editing field."


#. redo (text edit) command:
#. use | to separate multiple name values:
msgctxt "redo (text edit).names"
msgid "redo"
msgstr "ripeti"

msgctxt "redo (text edit).description"
msgid "Redoes your latest style/formatting or page-editing changes."
msgstr "Ripete l'ultima modifica applicata alla formattazione del testo o alla pagina."

msgctxt "redo (text edit).execute"
msgid "You're not in a rich text editing field."
msgstr You're not in a rich text editing field."


#. count words command:
#. use | to separate multiple name values:
msgctxt "count words.names"
msgid "count words|word count"
msgstr "quante parole|numero parole"

msgctxt "count words.description"
msgid "Displays the number of words in a selection."
msgstr "Mostra il numero di parole del testo selezionato."

msgctxt "count words.preview"
msgid "${num} words"
msgstr "${num} parole"

msgctxt "count words.preview"
msgid "Displays the number of words in a selection."
msgstr "Mostra il numero di parole in una selezione."

msgctxt "count words.execute"
msgid "${num} words"
msgstr "${num} parole"

msgctxt "count words.execute"
msgid "No words selected."
msgstr "Il testo selezionato non contiene alcuna parola."


#. link (to wikipedia) command:
#. use | to separate multiple name values:
msgctxt "link (to wikipedia).names"
msgid "link|insert link|linkify"
msgstr "rendi cliccabile"

msgctxt "link (to wikipedia).description"
msgid "Turns a phrase into a link to the matching Wikipedia article."
msgstr "Trasforma il testo selezionato in un collegamento a Wikipedia."


#. calculate command:
#. use | to separate multiple name values:
msgctxt "calculate.names"
msgid "calculate"
msgstr "calcola"

msgctxt "calculate.help"
msgid "Try it out: issue &quot;calc 22/7 - 1&quot;."
msgstr "Provare con &quot;calc 22/7 - 1&quot;."

msgctxt "calculate.description"
msgid "Calculates the value of a mathematical expression."
msgstr "Calcola il risultato di un'espressione matematica."

msgctxt "calculate.preview"
msgid "Calculates an expression. E.g., 22/7."
msgstr Valuta un'espressione matematica. Per esempio, 22/7."

msgctxt "calculate.preview"
msgid "${expression} = <b>${result}</b>{if error}<p><b>Error:</b> ${error}</p>{/if}"
msgstr "${expression} = <b>${result}</b>{if error}<p><b>Error:</b> ${error}</p>{/if}"

msgctxt "calculate.execute"
msgid "Requires a expression."
msgstr "È necessario specificare un'espressione matematica."

msgctxt "calculate.execute"
msgid "Invalid expression"
msgstr "Espressione non valida."

msgctxt "calculate.execute"
msgid "Error calculating expression: ${expression}"
msgstr "Si è verificato un errore cercando di valutare: ${expression}"


#. gcalculate command:
#. use | to separate multiple name values:
msgctxt "gcalculate.names"
msgid "gcalculate"
msgstr "gcalcola"

msgctxt "gcalculate.help"
msgid "Try 5% of 700,  sin( sqrt( ln(pi))),  (1+i)^3,  15 mod 9, (5 choose 2) / 3!,  speed of light in miles per hour,  3 dollars in euros,  242 in hex, MCMXVI in decimal."
msgstr"Provare con  5% of 700,  sin( sqrt( ln(pi))),  (1+i)^3,  15 mod 9, (5 choose 2) / 3!,  speed of light in miles per hour,  3 dollars in euros,  242 in hex, MCMXVI in decimal."

msgctxt "gcalculate.description"
msgid "Calculate knows many functions, constants, units, currencies, etc."
msgstr "Calculate knows many functions, constants, units, currencies, etc."

msgctxt "gcalculate.preview"
msgid "Examples: 3^4/sqrt(2)-pi,&nbsp;&nbsp;3 inch in cm,&nbsp;&nbsp; speed of light,&nbsp;&nbsp; 0xAF in decimal<br><u><a href=\\\"http://www.googleguide.com/calculator.html\\\">(Command List)</a></u>"
msgstr "Examples: 3^4/sqrt(2)-pi,&nbsp;&nbsp;3 inch in cm,&nbsp;&nbsp; speed of light,&nbsp;&nbsp; 0xAF in decimal<br><u><a href=\\\"http://www.googleguide.com/calculator.html\\\">(Command List)</a></u>"


#. sparkline command:
#. use | to separate multiple name values:
msgctxt "sparkline.names"
msgid "sparkline|graph|insert sparkline"
msgstr "inserisci sparkline"

msgctxt "sparkline.help"
msgid "Select a set of numbers -- in a table or otherwise -- and use this command to graph them as a sparkline. Don't worry about non-numbers getting in there. It'll handle them."
msgstr Select a set of numbers -- in a table or otherwise -- and use this command to graph them as a sparkline. Don't worry about non-numbers getting in there. It'll handle them."

msgctxt "sparkline.description"
msgid "Graphs the current selection, turning it into a sparkline."
msgstr Graphs the current selection, turning it into a sparkline."

msgctxt "sparkline.preview"
msgid "Requires numbers to graph."
msgstr "Richiede che vengano passati dei numeri per poter produrre un grafico."


#. translate command:
#. use | to separate multiple name values:
msgctxt "translate.names"
msgid "translate"
msgstr "traduci"

msgctxt "translate.help"
msgid "You can specify the language to translate to,\n"
"    and the language to translate from.\n"
"    For example, try issuing \"translate mother from english to chinese\".\n"
"    If you leave out the languages, it will try to guess what you want.\n"
"    It works on selected text in any web page,\n"
"    but there's a limit (a couple of paragraphs)\n"
"    to how much it can translate a selection at once.\n"
"    If you want to translate a lot of text, leave out the input and\n"
"    it will translate the whole page."
msgstr ""

msgctxt "translate.description"
msgid "Translates from one language to another."
msgstr "Traduce da una lingua ad un'altra."

msgctxt "translate.preview"
msgid "Replaces the selected text with the <b>${toLang}</b> translation:"
msgstr "Sostituisce la traduzione in <b>${toLang}</b> del testo selezionato: "


#. create bookmarklet command command:
#. use | to separate multiple name values:
msgctxt "create bookmarklet command.names"
msgid "create bookmarklet command"
msgstr "crea bookmarklet"

msgctxt "create bookmarklet command.description"
msgid "Creates a new command from a bookmarklet."
msgstr "Converte un bookmarklet in un comando."


#. create search command command:
#. use | to separate multiple name values:
msgctxt "create search command.names"
msgid "create search command"
msgstr "crea comando per la ricerca"

msgctxt "create search command.help"
msgid "<ol style=\"list-style-image:none\">\n"
"  <li>Select a searchbox.</li>\n"
"  <li>Execute this command to create the new search command.</li>\n"
"</ol>"
msgstr ""

msgctxt "create search command.description"
msgid "Creates a new Ubiquity command from a focused search-box."
msgstr "Crea un comando di ricerca a partire da un box di ricerca."

msgctxt "create search command.preview"
msgid "Creates a new search command called <b>${text}</b>"
msgstr ""

msgctxt "create search command.execute"
msgid "You need to focus a searchbox before running this command."
msgstr ""


